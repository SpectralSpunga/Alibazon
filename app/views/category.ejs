<!DOCTYPE html>
<html>
  <head lang="en">
    <title>Home</title>
    <link rel='stylesheet' type='text/css' href='/stylesheets/style.css' />
    <script scr="../controllers/homepageControlles.js"></script>
  </head>
  <body>
      <%- include('./header') %>

      <div id="navbar">
          <a href='#'>Home</a>
          <a href='/category/<%= mens[1].parent_category_id %>'><%= mainCategory %></a>
      </div>

      <main>
        <h2><%= mainCategory %></h2>
        <p><%= categoryDesc %></p>

        <% mens.forEach(function(elem){ %>
            <h3><%= elem.name %></h3>
            <img src="<%= elem.image %>" alt="<%= elem.id %>">
            <p><%= elem.page_description %></p>
            <button>View products</button>
            
            <hr>
        <% }); %>
      </main>

      <footer>Copyright 2020 OSF Digital</footer>
  </body>
</html>